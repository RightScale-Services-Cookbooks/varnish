#
#
[varnish]
name="Varnish Repo for Redhat/Centos"
baseurl=https://packagecloud.io/varnishcache/<%= @version %>/el/<%= @el_version %>/$basearch
repo_gpgcheck=1
gpgcheck=0
enabled=1
gpgkey=https://packagecloud.io/varnishcache/<%= @version %>/gpgkey
sslverify=1
sslcacert=/etc/pki/tls/certs/ca-bundle.crt
metadata_expire=300
